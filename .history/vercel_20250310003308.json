{
    "version":2,
    "builds":[
        {
            "src":"src/main.ts",
            "use":"@vercel/node",
            "config": {
                "buildCommand": "npm run build"
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"src/main.ts",
            "methods":[
                "GET",
                "POST",
                "PATCH",
                "DELETE"
            ]
        }
    ],
    
    "outputDirectory": "dist"
     
}